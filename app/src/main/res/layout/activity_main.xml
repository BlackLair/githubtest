<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:orientation="vertical"> //desktop의 주석문 주석문이다.
    //notebook 주석!!
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="마지막버튼"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World!"
        />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="이것은 b가 만든 버튼"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="호디ㅣ릴마ㅣ온"
        android:id="@+id/dsdsad" />
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="이름 입력하세요"
        android:id="@+id/edit_a"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="정우텍스트11"
        android:textSize="30dp"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="헬로 헬로"
        android:id="@+id/btnName"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="언제까지 버튼만들거야?"
        android:textSize="40dp"/>
    //dektop의 final 주석

    //desktop의 브랜치로 추가한 주석
</LinearLayout>